<service>
	<id>cardapioprinter.exe</id>
	<name>CardapioPrinter</name>
	<description>Cardapio 99Burger</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\HOME\AppData\Roaming\npm\node_modules\qckwinsvc\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>c:\Projetos\99Burger\backend\printer-service\service.js</argument>
	<argument>--log</argument>
